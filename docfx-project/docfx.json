{
  "metadata": [
    {
      "src": [
        {
          "files":"NativePlugins/IOSNativePro/**.cs",
          "src":"../../Plugins/Assets/Plugins/StansAssets/"
        }
      ],
      "dest": "ios-native"
    },
    {
      "src": [
        {
          "files":"NativePlugins/AndroidNativePro/**.cs",
          "src":"../../Plugins/Assets/Plugins/StansAssets/"
        }
      ],
      "dest": "android-native"
    },
     {
      "src": [
        {
          "files":"CrossPlatform/**.cs",
          "src":"../../Plugins/Assets/Plugins/StansAssets/"
        }
      ],
      "dest": "ultimate-mobile"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "ios-native/**.yml",
          "android-native/**.yml",
          "ultimate-mobile/**.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      }
    ],
    "dest": "../docs",
    "template": ["default", "templates/unity"]
  }
}
