{
  "metadata": [
    {
      "src": [
        {
          "files":"NativePlugins/IOSNativePro/**.cs",
          "src":"../../Plugins/Assets/Plugins/StansAssets/"
        }
      ],
      "dest": "ios-native"
    },
    {
      "src": [
        {
          "files":"NativePlugins/AndroidNativePro/**.cs",
          "src":"../../Plugins/Assets/Plugins/StansAssets/"
        }
      ],
      "dest": "android-native"
    },
     {
      "src": [
        {
          "files":[
              "UltimateMobile/Runtime/**.cs",
              "UltimateMobile/Editor/**.cs",
              "SAFacebook/Runtime/**.cs",
              "SocialGif/Runtime/**.cs",
          ],
          "src":"../../Plugins/Assets/Plugins/StansAssets/CrossPlatform/"
        }
      ],
      "dest": "ultimate-mobile"
    }
     ,
      {
       "src": [
         {
           "files":["Editor/**.cs", "Runtime/**.cs"],
           "src":"../../com.stansassets.foundation/"
         }
       ],
       "dest": "foundation"
     }
  ],
  "build": {
    "content": [
      {
        "files": [
          "ios-native/**.yml",
          "android-native/**.yml",
          "ultimate-mobile/**.yml",
          "foundation/**.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      }
    ],
    "dest": "../docs",
    "template": ["default", "templates/unity"]
  }
}
